

password1=typecho
password2=hedgedoc

# 安装 typecho 数据库

CREATE DATABASE typecho;
CREATE USER 'typecho'@'localhost' IDENTIFIED BY '$password1';
GRANT ALL PRIVILEGES ON typecho.* TO 'typecho'@'localhost';
FLUSH PRIVILEGES;

# 安装 hedgedoc 数据库

CREATE DATABASE hedgedoc;
CREATE USER 'hedgedoc'@'%' IDENTIFIED BY '$password2';
GRANT ALL PRIVILEGES ON hedgedoc.* TO 'hedgedoc'@'%';
FLUSH PRIVILEGES;
